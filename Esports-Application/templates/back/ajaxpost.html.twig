<tr {% for var in c %}>

<tr>
    <td>{{ var.id }}</td>
    <td>{{ var.sujet }}</td>
    <td>{{ var.description }}</td>
    <td>{{ var.communaute }}</td>



    {% if var.image is not null %}
        <td> <img src="{{ asset('/images/'~var.image) }}" alt="" width="150"></td>
    {%else  %}
        <td></td>
    {% endif %}



    <td><a href=" {{ path('dp',{'id':var.id}) }} " > <i class="fa-solid fa-trash-can"></i> </a></td>
    <td><a href=" {{ path('Upostback',{'id':var.id}) }} " > <i class="fa-solid fa-wrench"></i> </a></td>
    <td><a href="{{ path('Acob',{'id':var.id}) }}" ><i class="fa-solid fa-plus"></i> </a> </td>
    <td><a href="{{ path('affichbackcom',{'id':var.id}) }}" ><i class="fa-solid fa-eye"></i></a> </td>
</tr>








</tr {% endfor %}>