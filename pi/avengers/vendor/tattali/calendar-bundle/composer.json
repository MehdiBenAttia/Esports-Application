{
    "name": "tattali/calendar-bundle",
    "type": "symfony-bundle",
    "description": "Provides event calendar for your Symfony 5 project. Compatible with Doctrine ORM & ODM, and API like Google Calendar.",
    "keywords": [
        "fullcalendar",
        "calendar",
        "symfony",
        "symfony calendar"
    ],
    "homepage": "https://github.com/tattali/CalendarBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "ThÃ©o Attali",
            "email": "theoattali@gmail.com",
            "homepage": "https://theoattali.fr"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "symfony/dependency-injection": "^3.4 || ^4.4 || ^5.0 || ^6.0",
        "symfony/event-dispatcher": "^3.4 || ^4.4 || ^5.0 || ^6.0",
        "symfony/framework-bundle": "^3.4 || ^4.4 || ^5.0 || ^6.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpspec/prophecy": "^1.8",
        "phpunit/phpunit": "^7.5 | ^9.5",
        "symfony/dotenv": "^4.4 || ^5.0 || ^6.0",
        "symfony/phpunit-bridge": "^4.4 || ^5.0 || ^6.0",
        "symfony/yaml": "^4.4 || ^5.0 || ^6.0"
    },
    "suggest": {
        "symfony/orm-pack": "To support Doctrine ORM and Migration.",
        "doctrine/mongodb-odm-bundle": "To support MongoDB."
    },
    "autoload": {
        "psr-4": {
            "CalendarBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CalendarBundle\\Tests\\": "tests/"
        }
    }
}
